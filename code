#include <stdio.h>

int main()
{
    int prev, curr = 0;
    int sum = 0;
    float avg = 0;
    scanf("%d", &prev);
    while (curr != -1)
    {
        scanf("%d", &curr);
        if (curr != -1)
        {
            sum = prev + curr;
            avg = (float)sum / 2;
            printf("%.1f ", avg);
            prev = curr;
                
        }
    }
}
